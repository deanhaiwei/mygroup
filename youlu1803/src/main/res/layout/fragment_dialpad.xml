<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Fragment.DialpadFragment">

    <!-- TODO: Update blank fragment layout -->
    <include
        android:id="@+id/Dialpad_ActionBar"
        layout="@layout/actionbar_layout"/>
    <ListView
        style="@style/Style_ListView"
        android:layout_below="@+id/Dialpad_ActionBar"
        android:id="@+id/Dialpad_ListView"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </ListView>
    <RelativeLayout
        android:id="@+id/Dialpad_RelativeLayout_Keys"
        android:background="@mipmap/dialpad_bg"
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="300dp">
        <ImageButton
            android:id="@+id/Dialpad_CallPhone"
            android:layout_alignParentBottom="true"
            android:src="@mipmap/ic_call"
            android:layout_centerHorizontal="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </RelativeLayout>
</RelativeLayout>